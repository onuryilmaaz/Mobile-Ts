      },
      "package": "com.onur6541.salah",
      "navigationBar": {
        "backgroundColor": "#ffffff",
        "barStyle": "dark-content"
      }
    },
    "owner": "onur6541",
    "plugins": [
      "expo-asset",
      "expo-font",
      "expo-navigation-bar"
    ],
    "extra": {
      "expo-navigation-bar": {},
      "eas": {
        "projectId": "25385d71-03c1-4862-aeda-a8899b6d35d8"
      }
